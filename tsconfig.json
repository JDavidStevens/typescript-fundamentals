{
  "compilerOptions": {
    "noEmit": true,
    "strictNullChecks": true,
    "strict": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "target":"es6",
    "lib":["es5","dom"]
  },
  "include": ["examples/*/src", "challenges/*/src", "notes"]
}
